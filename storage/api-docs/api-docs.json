{
    "openapi": "3.0.0",
    "info": {
        "title": "Simple Bank",
        "description": "Api de Banco Simples",
        "contact": {
            "email": "seuemail@exemplo.com"
        },
        "version": "1.0"
    },
    "paths": {
        "/api/balance": {
            "get": {
                "tags": [
                    "balance"
                ],
                "summary": "realiza consulta de saldo bancario.",
                "operationId": "1e724cdabe1edbb59766478427db6f7c",
                "responses": {
                    "200": {
                        "description": "Status Ok"
                    },
                    "401": {
                        "description": "Status Unauthorized"
                    }
                }
            }
        },
        "/api/operation/deposit": {
            "post": {
                "tags": [
                    "balance"
                ],
                "summary": "realiza deposito de saldo.",
                "operationId": "075f98e8f5a35346367ead0cb01cbd34",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "amount"
                                ],
                                "properties": {
                                    "amount": {
                                        "type": "number",
                                        "format": "float",
                                        "example": "390.20"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Status Ok"
                    },
                    "401": {
                        "description": "Status Unauthorized"
                    }
                }
            }
        },
        "/api/login": {
            "post": {
                "tags": [
                    "user"
                ],
                "summary": "Realiza o login do usuário",
                "operationId": "c54022df2917d90ae55652ee33ccf2b9",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "joao@email.com"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login Ok"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/api/register": {
            "post": {
                "tags": [
                    "user"
                ],
                "summary": "Realiza o cadastro de usários",
                "operationId": "dcbd07d28fe60878864011b9fd551690",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "first_name",
                                    "last_name",
                                    "email",
                                    "password",
                                    "user_type",
                                    "document"
                                ],
                                "properties": {
                                    "first_name": {
                                        "type": "string",
                                        "example": "Jonh"
                                    },
                                    "last_name": {
                                        "type": "string",
                                        "example": "Doe"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "joao@email.com"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "user_type": {
                                        "type": "string",
                                        "example": "common"
                                    },
                                    "document": {
                                        "type": "string",
                                        "example": "417.792.460-13"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User Succefully Created"
                    },
                    "400": {
                        "description": "Error"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "balance",
            "description": "balance"
        },
        {
            "name": "user",
            "description": "user"
        }
    ]
}